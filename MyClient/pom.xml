	<modelVersion>4.0.0</modelVersion>
	<groupId>cpcn.xyt</groupId>
	<artifactId>MyClient</artifactId>
	<version>1.0-SNAPSHOT</version>


	<properties>
		<spring.version>3.2.18.RELEASE</spring.version>
